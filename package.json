{
  "name": "contentful-node",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.7",
    "eslint": "^6.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "contentful-management": "6.2.0-beta0",
    "express": "^4.17.1",
    "tsc-watch": "^4.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
